{
  "name": "simple-web-ui-tester-admin",
  "version": "1.0.0",
  "description": "Simple UI using npm simple-web-ui-tester to create scripts test",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "serve": "cross-env run-p serve-server serve-client",
    "serve-server": "nodemon index.js",
    "serve-client": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linhnt2803/simple-web-ui-tester-admin.git"
  },
  "keywords": [
    "simple-web-ui-tester-admin",
    "simple-web-ui-tester",
    "web-ui-tester",
    "test",
    "web",
    "ui"
  ],
  "author": "LT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/linhnt2803/simple-web-ui-tester-admin/issues"
  },
  "homepage": "https://github.com/linhnt2803/simple-web-ui-tester-admin#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "simple-web-ui-tester": "^1.1.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.0.0-beta.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "sass": "^1.32.2",
    "sass-loader": "^10.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "nodemonConfig": {
    "ignore": [
      "public",
      "src"
    ]
  },
  "pre-commit": [
    "build"
  ]
}
